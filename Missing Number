#include <stdio.h>

// Creating the "in" operator from Python.
int in(int element, int array[], int size) {

    # Functionality of Python for loop: For i in range(0, len(array):
    for (int i = 0; i < size; i++) {
        if (array[i] == element) {
            return 1;
        }
    }
    return 0;
}

int missing_number(int nums[], int size) {
    for (int i = 0; i < size; i++) {
        if (!in(i, nums, size)) {
            return i;
        }
    }
    return -1;
}

int main() {
    int list[] = {0, 1, 2, 3, 5};

    int result = missing_number(list, 5);

    printf("%d", result);

    return 0;
}
